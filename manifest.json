{
        "manifest_version": 3,
        "name": "Redundant Tab Remover",
        "description": "Chrome extension that removes redundant tabs.",
        "version": "1.0",
        "permissions": ["storage", "tabs", "browsingData"],
        "action": {
            "default_popup": "popup.html"
       
        },
        "background": {
            "service_worker": "background.js"
        }
        
}
